{
  "name": "MyContract",
  "contract": {
    "variables": [
      {
        "type": "address",
        "name": "manager",
        "scope": "public",
        "value": "",
        "setMethod": false
      },
      {
        "type": "string",
        "name": "name",
        "scope": "public",
        "value": "",
        "setMethod": true
      },
      {
        "type": "uint256",
        "name": "value1",
        "scope": "public",
        "value": "",
        "setMethod": false
      },
      {
        "type": "uint256",
        "name": "max_value1",
        "scope": "public",
        "value": "",
        "setMethod": false
      },
      {
        "type": "uint256",
        "name": "min_value1",
        "scope": "public",
        "value": "",
        "setMethod": false
      }
    ],
    "functions": [
      {
        "name": "",
        "scope": "public",
        "isConstructor": true,
        "inputs": [{ "name": "_owner", "type": "address" }],
        "outputs": [],
        "content": [
          {
            "statement": "assignment",
            "variable": "manager",
            "value": "_owner"
          },
          {
            "statement": "assignment",
            "variable": "name",
            "value": "DHT11"
          }
        ]
      },
      {
        "name": "setValue",
        "scope": "public",
        "isConstructor": false,
        "inputs": [
          {
            "name": "_val",
            "type": "uint256"
          },
          {
            "name": "_valueId",
            "type": "uint256"
          }
        ],
        "outputs": [],
        "content": [
          {
            "statement": "if",
            "condition": "valueId == 1",
            "content": [
              {
                "statement": "assignment",
                "variable": "value1",
                "value": "_val"
              },
              {
                "statement": "if",
                "condition": "value1 >= max_value1",
                "content": [
                  {
                    "statement": "callevent",
                    "name": "temperatureOverflow",
                    "inputs": [
                      {
                        "name": "value1",
                        "type": "uint256"
                      },
                      {
                        "name": "max_value1",
                        "type": "uint256"
                      }
                    ]
                  }
                ]
              },
              {
                "statement": "if",
                "else": true,
                "condition": "value1 <= min_value1",
                "content": [
                  {
                    "statement": "callevent",
                    "name": "temperatureUnderflow",
                    "inputs": [
                      {
                        "name": "value1",
                        "type": "uint256"
                      },
                      {
                        "name": "min_value1",
                        "type": "uint256"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
}
