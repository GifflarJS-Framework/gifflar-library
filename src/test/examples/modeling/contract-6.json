  {
    "name": "DHT11Controller",
    "contract": {
      "variables": [
        {
          "type": "address[]",
          "name": "contracts",
          "scope": "public",
          "value": "",
          "setMethod": false
        },
        {
          "type": "uint",
          "name": "counter",
          "scope": "private",
          "value": 0,
          "setMethod": false
        }
      ],
      "functions": [
        {
          "name": "createContract",
          "scope": "public",
          "isConstructor": false,
          "inputs": [
            {
              "name": "_owner",
              "type": "address"
            }
          ],
          "outputs": [],
          "content": [
            {
              "statement": "variable",
              "type": "address",
              "name": "newContract",
              "value": "new DHT11(_owner)"
            },
            {
              "statement": "callmethod",
              "variable": "contracts",
              "method": "push",
              "value": "newContract"
            },
            {
              "statement": "assignment",
              "variable": "counter",
              "value": "counter + 1"
            }
          ]
        },
        {
          "name": "getLastContract",
          "scope": "public",
          "isConstructor": false,
          "inputs": [],
          "outputs": ["lastContract"],
          "content": [
            {
              "statement": "variable",
              "type": "address",
              "name": "lastContract",
              "value": "address(0)"
            },
            {
              "statement": "if",
              "else": false,
              "condition": "counter > 0",
              "content": [
                {
                  "statement": "assignment",
                  "variable": "lastContract",
                  "value": "contracts[counter - 1]"
                }
              ]
            },
            {
              "statement": "if",
              "else": true,
              "condition": "",
              "content": [
                {
                  "statement": "assignment",
                  "variable": "lastContract",
                  "value": "contracts[0]"
                }
              ]
            }
          ]
        }
      ]
    }
  }
